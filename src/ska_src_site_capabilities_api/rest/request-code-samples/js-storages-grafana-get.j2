const http = require("http");

const options = {
  "method": "GET",
  "hostname": "localhost",
  "port": "8080",
  "path": "/v1/storages/grafana?node_names=SOME_STRING_VALUE&site_names=SOME_STRING_VALUE&include_inactive=SOME_BOOLEAN_VALUE",
  "headers": {}
};

const req = http.request(options, function (res) {
  const chunks = [];

  res.on("data", function (chunk) {
    chunks.push(chunk);
  });

  res.on("end", function () {
    const body = Buffer.concat(chunks);
    console.log(body.toString());
  });
});

req.end();