package main

import (
	"fmt"
	"net/http"
	"io/ioutil"
)

func main() {

	url := "{{ api_server_url }}/sites?only_node_names=SOME_STRING_VALUE&include_inactive=SOME_BOOLEAN_VALUE"

	req, _ := http.NewRequest("GET", url, nil)

	res, _ := http.DefaultClient.Do(req)

	defer res.Body.Close()
	body, _ := ioutil.ReadAll(res.Body)

	fmt.Println(res)
	fmt.Println(string(body))

}