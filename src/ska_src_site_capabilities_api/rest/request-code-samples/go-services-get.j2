package main

import (
	"fmt"
	"net/http"
	"io/ioutil"
)

func main() {

	url := "{{ api_server_url }}/services?node_names=SOME_STRING_VALUE&site_names=SOME_STRING_VALUE&service_types=SOME_STRING_VALUE&service_scope=SOME_STRING_VALUE&include_inactive=SOME_BOOLEAN_VALUE&associated_storage_area_id=SOME_STRING_VALUE"

	req, _ := http.NewRequest("GET", url, nil)

	req.Header.Add("Authorization", "Bearer REPLACE_BEARER_TOKEN")

